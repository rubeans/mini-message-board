<%- include("./layout.ejs") %>
  <h1><%= title %></h1>
  <button><a href="/new" target="_self" rel="noopener noreferrer">New Message</a></button>
  <% messages.forEach(message =>{ %>
  <ul>
    <li>Message: "<%= message.text %>" - By: <%= message.user %> - Date: <%= message.added %></li>
  </ul>
  <% }) %>
</body>

</html>